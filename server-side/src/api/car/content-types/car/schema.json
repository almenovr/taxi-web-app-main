{
  "kind": "collectionType",
  "collectionName": "cars",
  "info": {
    "singularName": "car",
    "pluralName": "cars",
    "displayName": "Car",
    "description": "Автомобили для такси"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "className": {
      "type": "string",
      "required": true
    },
    "listCars": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": false
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "isBusiness": {
      "type": "boolean",
      "default": false
    },
    "pricePerKm": {
      "type": "decimal",
      "required": true
    },
    "carImg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "destinations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "inversedBy": "cars"
    }
  }
}
